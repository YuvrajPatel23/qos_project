{
  "logs": {
    "logs_collected": {
      "files": {
        "collect_list": [
          {
            "file_path": "/home/ubuntu/qos-project/qos-logs/qos_main.log",
            "log_group_name": "/qos/main",
            "log_stream_name": "{instance_id}-main",
            "timezone": "UTC",
            "multi_line_start_pattern": "{timestamp}"
          },
          {
            "file_path": "/home/ubuntu/qos-project/qos-logs/pipeline.log", 
            "log_group_name": "/qos/pipeline",
            "log_stream_name": "{instance_id}-pipeline",
            "timezone": "UTC"
          },
          {
            "file_path": "/home/ubuntu/qos-project/qos_results.csv",
            "log_group_name": "/qos/results",
            "log_stream_name": "{instance_id}-results",
            "timezone": "UTC"
          }
        ]
      }
    }
  },
  "metrics": {
    "namespace": "QoS/Performance",
    "metrics_collected": {
      "cpu": {
        "measurement": ["cpu_usage_idle", "cpu_usage_iowait"],
        "metrics_collection_interval": 60
      },
      "disk": {
        "measurement": ["used_percent"],
        "metrics_collection_interval": 60,
        "resources": ["/"]
      },
      "mem": {
        "measurement": ["mem_used_percent"],
        "metrics_collection_interval": 60
      }
    }
  }
}